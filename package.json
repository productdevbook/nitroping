{
  "name": "@nitroping/app",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "release": "bun run build && bun publish --no-git-checks --access public",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate --config ./server/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./server/drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config ./server/drizzle.config.ts",
    "db:up": "drizzle-kit up --config ./server/drizzle.config.ts",
    "db:check": "drizzle-kit check --config ./server/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./server/drizzle.config.ts",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@tanstack/vue-table": "^8.21.3",
    "@types/jsonwebtoken": "^9.0.10",
    "abckit": "^0.0.28",
    "bullmq": "^5.66.0",
    "dataloader": "^2.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "graphql": "^16.12.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "nitroping": "file:./sdk",
    "nuxt": "^4.2.2",
    "postgres": "^3.4.7",
    "undici": "^7.16.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@types/three": "^0.182.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
